pipeline{
 agent any
		stages{
			
		 

         stage('save log build') {
				    steps {
				     echo "aaaaaaaaaaaaaaaaaaaaaaaaa"
				    echo env.JOB_NAME
				    
				     echo job
				     echo buildNum
				     echo "bbbbb"
						      
						        echo "111111111111111111111"
						        echo build.project.name
						        echo build.project.buildnumber
						          echo "2222222222222222222222"
						          script{
						            def logContent = Jenkins.getInstance().getItemByFullName(System.getenv("JOB_NAME")).getBuildByNumber(Integer.parseInt(System.getenv("BUILD_NUMBER"))).logFile.text
						            // copy the log in the job's own workspace
						            writeFile file: "buildlog.txt", text: logContent
						            }
						      
				    }
			}
   }	  

 
    
}		  