pipeline{
 agent any
		stages{
			
		 

		         stage('save log build') {
						    steps {
						     echo "aaaaaaaaaaaaaaaaaaaaaaaaa"
						    echo env.JOB_NAME
						    
						    
						     echo "bbbbb"
								      
								        echo "111111111111111111111a"
								      
								       echo  ${JENKINS_HOME}
								        echo "111111111111111111111.11111"
								       echo  ${JENKINS_HOME}/jobs/${JOB_NAME}/builds/${BUILD_NUMBER}/log >> log.txt
								       
								       //cat ${JENKINS_HOME}/jobs/${JOB_NAME}/builds/${BUILD_NUMBER}/log >> log.txt
								       
								        
								      
						    
					           }
		           }
           }
   
   post {
        always {
            archiveArtifacts artifacts: 'target/*', onlyIfSuccessful: false
        }
    }
    	  

 
    
}		  