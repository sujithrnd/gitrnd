pipeline{
 agent any
		stages{
			
		 

		         stage('save log build') {
						    steps {
						     echo "aaaaaaaaaaaaaaaaaaaaaaaaa"
						    echo env.JOB_NAME
						    
						    
						     echo "bbbbb"
								      
								        echo "111111111111111111111b"
								      
								       echo  env.JENKINS_HOME
								        echo "111111111111111111111.11111"
								       echo  env.JENKINS_HOME/jobs/env.JOB_NAME/builds/env.BUILD_NUMBER/log >> log.txt
								       
								      
								       
								        echo "22222222"
								      
						    
					           }
		           }
           }
   
   post {
        always {
            archiveArtifacts artifacts: 'target/*', onlyIfSuccessful: false
        }
    }
    	  

 
    
}		  